{
  "name": "@lajemacarts/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "serve_lajemacarts": "nx serve lajemacarts -o --port=4200",
    "build_lajemacarts": "nx build lajemacarts",
    "serve_gallery": "nx serve gallery -o --port=4201",
    "build_gallery": "nx build gallery",
    "serve_dashboard": "nx serve dashboard -o --port=4202",
    "build_dashboard": "nx build dashboard ",
    "start:artwork-service": "nx serve artwork-service",
    "graph-affected-projects": "nx graph --affected",
    "del-dist": "rmdir /s /q dist",
    "clean-node_modules": "del /f /q package-lock.json & rmdir /s /q node_modules",
    "clean-cache-angular": "if exist .angular rmdir /s /q .angular",
    "clean-cache-nx": "nx reset",
    "clean-all-caches": "npm cache clean --force && npm run clean-cache-angular && npm run clean-cache-nx",
    "clean-project": "npm run clean-node_modules && npm run clean-cache-angular && npm run del-dist",
    "clean-all": "npm run clean-node_modules && npm run clean-all-caches"
  },
  "private": true,
